{
    "name": "thiagoprz/braspag-laravel",
    "description": "Biblioteca desenvolvida para dar suporte no Laravel as formas de pagamento Braspag.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Thiagoprz\\Braspag\\": "src"
        }
    },
    "authors": [
        {
            "name": "Thiago Przyczynski",
            "email": "przyczynski@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^6.3||^7.0.1",
        "php": "^7.1.3||^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thiagoprz\\Braspag\\BraspagServiceProvider"
            ]
        }
    }
}
